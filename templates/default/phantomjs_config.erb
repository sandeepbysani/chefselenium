{
  "webdriver": "<%= @resource.webdriver %>",
  <% if @resource.webdriverSeleniumGridHub %>
  "webdriverSeleniumGridHub": "<%= @resource.webdriverSeleniumGridHub %>",
  <% end %>
  "cookiesFile": "<%= @resource.cookiesFile %>",
  "diskCacheEnabled": "<%= @resource.diskCacheEnabled %>",
  "ignoreSslErrors": "<%= @resource.ignoreSslErrors %>",
  "autoLoadImages": "<%= @resource.autoLoadImages %>",
  "offlineStoragePath": "<%= @resource.offlineStoragePath %>",
  "offlineStorageDefaultQuota": "<%= @resource.offlineStorageDefaultQuota %>",
  "localToRemoteUrlAccessEnabled": "<%= @resource.localToRemoteUrlAccessEnabled %>",
  "maxDiskCacheSize": "<%= @resource.maxDiskCacheSize %>",
  "outputEncoding": "<%= @resource.outputEncoding %>",
  "remoteDebuggerPort": "<%= @resource.remoteDebuggerPort %>",
  "remoteDebuggerAutorun": "<%= @resource.remoteDebuggerAutorun %>",
  "proxy": "<%= @resource.proxy %>",
  "proxyType": "<%= @resource.proxyType %>",
  "proxyAuth": "<%= @resource.proxyAuth %>",
  "scriptEncoding": "<%= @resource.scriptEncoding %>",
  "sslProtocol": "<%= @resource.sslProtocol %>",
  "sslCertificatesPath": "<%= @resource.sslCertificatesPath %>",
  "webSecurityEnabled": "<%= @resource.webSecurityEnabled %>"
}
